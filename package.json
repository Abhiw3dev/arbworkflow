{
  "dependencies": {
    "clipboard": "^2.0.11",
    "clipboardy": "^2.3.0",
    "dotenv": "^16.4.5",
    "fs": "^0.0.1-security",
    "jest-puppeteer": "^10.0.1",
    "moment-timezone": "^0.5.45",
    "puppeteer": "^21.10.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "scripts": {
    "start": "node test/arbSearch.js",
    "arb": "node src/arbScript.js ",
